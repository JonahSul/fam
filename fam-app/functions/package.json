    "@genkit-ai/firebase": "^0.5.0",
    "@genkit-ai/googleai": "^0.5.0",
    "firebase-admin": "^12.3.0",
    "firebase-functions": "^5.1.0",
    "genkit": "^0.5.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "typescript": "^5.4.5"
  },
  "private": true
}
