# Environment Configuration Template
# Copy this file to .env and fill in your actual values

# Anthropic API Key
# Obtain from https://console.anthropic.com/
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# GenKit Chat Service Endpoints
# These are consumed via --dart-define at build time
CHAT_SERVICE_URL=http://localhost:3000
CHAT_SERVICE_URL_ANDROID=http://10.0.2.2:3000
CHAT_SERVICE_URL_IOS=http://localhost:3000
CHAT_SERVICE_URL_DESKTOP=http://localhost:3000

# Firebase Configuration
# These values come from your Firebase project settings
FIREBASE_PROJECT_ID=your_firebase_project_id
FIREBASE_API_KEY=your_firebase_api_key
FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
FIREBASE_MESSAGING_SENDER_ID=your_sender_id
FIREBASE_APP_ID=your_app_id

# Web specific
FIREBASE_MEASUREMENT_ID=your_measurement_id

# Android specific
FIREBASE_ANDROID_APP_ID=your_android_app_id

# iOS specific
FIREBASE_IOS_APP_ID=your_ios_app_id
FIREBASE_IOS_BUNDLE_ID=com.example.montanagent